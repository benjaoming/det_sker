		<div class="day row">
			<div class="col-xs-12">
				<h2>
					<%= format_date(day.date) %>
  					(<%= day.events.length %>)
				</h2>
			</div>
			<div class="row">
				<div class="events-by-date_js col-xs-12">
					<div class="row">
 						<% day.events.each do |event| %>
  						<%= render partial: 'events/event', object: event %>
						<% end %>
					</div>
				</div>
			</div>
		</div>